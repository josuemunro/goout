 <div class="headercontainer">
        <img class="mountainimg" src="images/mtcookcrop.jpg">
        <h1 class="logintitle">Go Out</h1>
        <h2 class="signUp">Sign Up</h2>
        <h2 class="logIn">Log In</h2>
    </div>
    <div id="formscontainer">
        <div id=signUp>
            <form method="post" action="/signup">
                <input type="text" name="firstName" placeholder="first name">
                <input type="text" name="lastName" placeholder="last name">
                <input type="number" name="age" placeholder="age">
                <input type="email" name="email" placeholder="email">
                <input type="number" name="phoneNumber" placeholder="mobile number">
                <input type="password" name="password" placeholder="password">
                <input type="password" name="retypepassword" placeholder="retype password">
                <button type="submit">Sign up!</button>
            </form>
        </div>
        <div id="logIn">
            <form method="post" action="/login">
                <input type="text" name="email" placeholder="email">
                <input type="password" name="password" placeholder="password">
                <button type="submit">Log in!</button>
            </form>
        </div>
    </div>